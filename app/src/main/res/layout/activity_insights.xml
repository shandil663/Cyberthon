<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    tools:context=".insights">

    <TextView
        android:id="@+id/insightheading"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:gravity="center_horizontal"
        android:text="Choose Category"
        android:textAlignment="center"
        android:textColor="@color/black"
        android:textSize="25sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.022" />

    <androidx.cardview.widget.CardView
        android:id="@+id/phish1"
        android:layout_width="190dp"
        android:layout_height="170sp"
        app:cardCornerRadius="20sp"
        app:cardElevation="5sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.031"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.139">

        <ImageView
            android:id="@+id/imageView3"
            android:layout_width="150sp"
            android:layout_height="110sp"
            android:layout_gravity="center_horizontal"
            android:layout_marginTop="10sp"
            app:srcCompat="@drawable/img_16" />

        <TextView
            android:id="@+id/phish"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal|bottom"
            android:layout_marginBottom="20sp"
            android:text="Phishing"
            android:textAlignment="center"
            android:textSize="15sp"
            android:textStyle="normal"

            />
    </androidx.cardview.widget.CardView>
    <androidx.cardview.widget.CardView
        android:id="@+id/evicard1"
        android:layout_width="190dp"
        android:layout_height="170sp"
        app:cardCornerRadius="20sp"
        app:cardElevation="5sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.031"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.402">

        <ImageView
            android:id="@+id/evi"
            android:layout_width="170sp"
            android:layout_height="120sp"
            android:layout_gravity="center_horizontal"
            android:layout_marginTop="10sp"
            app:srcCompat="@drawable/img_18" />

        <TextView
            android:id="@+id/bullying"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal|bottom"
            android:layout_marginBottom="15sp"
            android:text="Cyberbullying"
            android:textAlignment="center"
            android:textSize="15sp"
            android:textStyle="normal"

            />
    </androidx.cardview.widget.CardView>

    <androidx.cardview.widget.CardView
        android:id="@+id/assignedcasescard"
        android:layout_width="190dp"
        android:layout_height="170sp"
        app:cardCornerRadius="20sp"
        app:cardElevation="5sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.031"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.667">

        <ImageView
            android:id="@+id/assigned"
            android:layout_width="170sp"
            android:layout_height="120sp"
            android:layout_gravity="center_horizontal"
            android:layout_marginTop="10sp"
            app:srcCompat="@drawable/img_21" />

        <TextView
            android:id="@+id/assignedcases"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal|bottom"
            android:layout_marginBottom="15sp"
            android:text="Spyware"
            android:textAlignment="center"
            android:textSize="15sp"
            android:textStyle="normal"

            />
    </androidx.cardview.widget.CardView>

    <androidx.cardview.widget.CardView
        android:id="@+id/caseinsights"
        android:layout_width="190dp"
        android:layout_height="170sp"
        app:cardCornerRadius="20sp"
        app:cardElevation="5sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.031"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.932">

        <ImageView
            android:id="@+id/insight"
            android:layout_width="170sp"
            android:layout_height="120sp"
            android:layout_gravity="center_horizontal"
            android:layout_marginTop="10sp"
            app:srcCompat="@drawable/img_23" />

        <TextView
            android:id="@+id/casedata"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal|bottom"
            android:layout_marginBottom="15sp"
            android:text="Cyberterrorism"
            android:textAlignment="center"
            android:textSize="15sp"
            android:textStyle="normal"

            />
    </androidx.cardview.widget.CardView>

    <androidx.cardview.widget.CardView
        android:id="@+id/chatcard1"
        android:layout_width="190dp"
        android:layout_height="170sp"
        app:cardCornerRadius="20sp"
        app:cardElevation="5sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.959"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.402">

        <ImageView
            android:id="@+id/chat"
            android:layout_width="170sp"
            android:layout_height="120sp"
            android:layout_gravity="center_horizontal"
            android:layout_marginTop="10sp"
            app:srcCompat="@drawable/img_20" />

        <TextView
            android:id="@+id/chat1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal|bottom"
            android:layout_marginBottom="15sp"
            android:text="Fraud"
            android:textAlignment="center"
            android:textSize="15sp"
            android:textStyle="normal"

            />
    </androidx.cardview.widget.CardView>

    <androidx.cardview.widget.CardView
        android:id="@+id/emergencycard"
        android:layout_width="190dp"
        android:layout_height="170sp"
        app:cardCornerRadius="20sp"
        app:cardElevation="5sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.959"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.667">

        <ImageView
            android:id="@+id/emergencyimg"
            android:layout_width="170sp"
            android:layout_height="120sp"
            android:layout_gravity="center_horizontal"
            android:layout_marginTop="10sp"
            app:srcCompat="@drawable/img_22" />

        <TextView
            android:id="@+id/Emergency"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal|bottom"
            android:layout_marginBottom="15sp"
            android:text="Cryptojacking"
            android:textAlignment="center"
            android:textSize="15sp"
            android:textStyle="normal"

            />
    </androidx.cardview.widget.CardView>

    <androidx.cardview.widget.CardView
        android:id="@+id/Resources"
        android:layout_width="190dp"
        android:layout_height="170sp"
        app:cardCornerRadius="20sp"
        app:cardElevation="5sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.959"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.932">

        <ImageView
            android:id="@+id/train"
            android:layout_width="170sp"
            android:layout_height="120sp"
            android:layout_gravity="center_horizontal"
            android:layout_marginTop="10sp"
            app:srcCompat="@drawable/img_24" />

        <TextView
            android:id="@+id/Train"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal|bottom"
            android:layout_marginBottom="15sp"
            android:text="Security Hacker"
            android:textAlignment="center"
            android:textSize="15sp"
            android:textStyle="normal"

            />
    </androidx.cardview.widget.CardView>

    <androidx.cardview.widget.CardView
        android:id="@+id/casecard"
        android:layout_width="190dp"
        android:layout_height="170sp"
        app:cardCornerRadius="20sp"
        app:cardElevation="5sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.959"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.139">


        <ImageView
            android:id="@+id/imagview"
            android:layout_width="170sp"
            android:layout_height="120sp"
            android:layout_gravity="center_horizontal"
            android:layout_marginTop="10sp"
            app:srcCompat="@drawable/img_17" />

        <TextView
            android:id="@+id/malware"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal|bottom"
            android:layout_marginBottom="20sp"
            android:text="Malware"
            android:textAlignment="center"
            android:textSize="15sp"
            android:textStyle="normal"

            />
    </androidx.cardview.widget.CardView>


</androidx.constraintlayout.widget.ConstraintLayout>
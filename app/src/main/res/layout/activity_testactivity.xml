<?xml version="1.0" encoding="utf-8"?>
<ScrollView
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:layout_editor_absoluteX="1dp"
    tools:context=".Testactivity"
    tools:layout_editor_absoluteY="1dp">
<LinearLayout
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    >
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:layout_margin="20sp"
       android:gravity="center">

        <TextView
            android:id="@+id/textincident"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Cyber Crime Branch"
            android:layout_marginStart="42sp"
            android:textAlignment="center"
            android:textColor="@color/black"
            android:textSize="25sp"
            android:textStyle="bold"/>

        <androidx.cardview.widget.CardView
            android:id="@+id/profile"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:cardCornerRadius="50sp"
            app:cardElevation="5sp"
            android:layout_marginStart="50sp"
            >

            <ImageView
                android:id="@+id/imageView12"
                android:layout_width="30dp"
                android:layout_height="30sp"
                android:layout_gravity="center"
                android:layout_margin="10sp"
                android:src="@drawable/img_2" />
        </androidx.cardview.widget.CardView>
    </LinearLayout>

    <LinearLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_weight="1">

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:gravity="center"
            >
            <androidx.cardview.widget.CardView
                android:id="@+id/newcard"
                android:layout_width="190dp"
                android:layout_height="170sp"
             android:layout_margin="5sp"
                app:cardCornerRadius="20sp"
                app:cardElevation="5sp"
                >

                <ImageView
                    android:id="@+id/imageView3"
                    android:layout_width="150sp"
                    android:layout_height="110sp"
                    android:layout_gravity="center_horizontal"
                    android:layout_marginTop="10sp"
                    app:srcCompat="@drawable/img_3" />

                <TextView
                    android:id="@+id/textView2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_horizontal|bottom"
                    android:layout_marginBottom="20sp"
                    android:text="File new Case"
                    android:textAlignment="center"
                    android:textSize="15sp"
                    android:textStyle="normal"

                    />
            </androidx.cardview.widget.CardView>

            <androidx.cardview.widget.CardView
                android:id="@+id/casecard"
                android:layout_width="190dp"
                android:layout_height="170sp"
                app:cardCornerRadius="20sp"
                app:cardElevation="5sp"
               >


                <ImageView
                    android:id="@+id/imagview"
                    android:layout_width="170sp"
                    android:layout_height="120sp"
                    android:layout_gravity="center_horizontal"
                    android:layout_marginTop="10sp"
                    app:srcCompat="@drawable/img_4" />

                <TextView
                    android:id="@+id/textviewh"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_horizontal|bottom"
                    android:layout_marginBottom="20sp"
                    android:text="Check Case Status"
                    android:textAlignment="center"
                    android:textSize="15sp"
                    android:textStyle="normal"

                    />
            </androidx.cardview.widget.CardView>
        </LinearLayout>

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:gravity="center"
            >
            <androidx.cardview.widget.CardView
                android:id="@+id/evicard1"
                android:layout_width="190dp"
                android:layout_height="170sp"
                android:layout_margin="5sp"
